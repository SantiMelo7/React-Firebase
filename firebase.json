{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "us-east1"
    }
  },
  "rewrites": [
    {
      "source": "**",
      "detination": "/index.html"
    }
  ],
  "predeploy": [
    "npm run build"
  ]
}
